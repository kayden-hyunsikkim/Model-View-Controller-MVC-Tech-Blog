<!-- Loop over each object in the projects array -->
{{#each projects as |tech| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/project/{{tech.id}}">{{tech.name}}</a>
    </h2>
    <p>Created by {{tech.user.name}} on {{format_date tech.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{tech.description}}
    </p>
  </div>
</div>
{{/each}}
